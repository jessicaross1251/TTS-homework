   "source": [
    "from re import fullmatch\n",
    "from email.utils import parseaddr\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "for _ in range(N):\n",
    "    input_line = input()\n",
    "\n",
    "    if fullmatch(r\"[a-zA-Z][a-zA-Z0-9_\\-\\.]+@[a-zA-Z]+\\.[a-zA-Z]{1,3}\", parseaddr(input_line)[1]):\n",
    "        print(input_line)"
   ]
  }
